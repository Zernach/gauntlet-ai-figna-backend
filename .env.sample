# ============================================
# CollabCanvas Backend - Environment Variables
# ============================================
#
# Copy this file to .env and fill in your actual values
# Never commit .env to version control!
#
# Get Supabase credentials from:
# https://supabase.com/dashboard → Your Project → Project Settings → API
#

# ============================================
# Server Configuration
# ============================================
NODE_ENV=development
PORT=3001
HOST=0.0.0.0

# ============================================
# CORS Configuration
# ============================================
# Comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:8081,http://localhost:19006,http://localhost:19000

# ============================================
# Supabase Configuration
# ============================================
# Get these from: Project Settings → API

# Project URL (e.g., https://xxxxx.supabase.co)
SUPABASE_URL=your-project-url-here

# Service Role Key (⚠️ SECRET - use in backend only!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Anon/Public Key (safe to use in frontend)
SUPABASE_ANON_KEY=your-anon-key-here

# JWT Secret (found in Project Settings → API → JWT Secret)
SUPABASE_JWT_SECRET=your-jwt-secret-here

# ============================================
# WebSocket Configuration
# ============================================
WS_PORT=3002
WS_HEARTBEAT_INTERVAL=30000
WS_MAX_PAYLOAD=10485760

# ============================================
# Session Configuration
# ============================================
SESSION_SECRET=change-this-to-a-random-string-in-production

# ============================================
# Logging
# ============================================
# Options: debug, info, warn, error
LOG_LEVEL=info

# ============================================
# Performance Settings
# ============================================
MAX_OBJECTS_PER_CANVAS=1000
PRESENCE_TTL_SECONDS=30

# ============================================
# Rate Limiting
# ============================================
# Window in milliseconds (default: 15 minutes)
RATE_LIMIT_WINDOW_MS=900000
# Max requests per window
RATE_LIMIT_MAX_REQUESTS=100



GAUNTLET_OPEN_AI_API_KEY_1=
GAUNTLET_OPEN_AI_API_KEY_2=